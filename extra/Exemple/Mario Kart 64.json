    "[Complete-BFrancois]MARIOKART64_HIRESTEXTURES.hts": {
        "file": "Mario Kart 64\/[Complete-BFrancois]MARIOKART64_HIRESTEXTURES.hts",
        "file_sha": "8B417DDE068FF089E9F453ACC3D1237B34E2F066",
        "file_size": 1619533755,
        "texture": {
            "name": "[Complete-BFrancois]MARIOKART64_HIRESTEXTURES.hts",
            "sha": "8B417DDE068FF089E9F453ACC3D1237B34E2F066",
            "url": "https:\/\/emulationking.com\/bfrancois-mario-kart-64\/",
            "tag": "Complete-BFrancois",
            "type": "Complete",
            "info_tag": "BFrancois",
            "rom_specific": [],
            "txt": "BFrancois Mario Kart 64 texture pack is a superb retexturing attempt, replacing the style and feel of the game with one more reminiscent of the original SNES Mario Kart game.\r\n\r\nThis project originally started off as just a replacement for the kart textures.\r\n\r\nHowever BFrancois felt that the pixelated course textures that are in Mario Kart 64 kind of took away from all his hard work so began work on retexturing every world texture.\r\n\r\nThe ultimate goal of the texture pack was to replace every Super Mario 64 texture with one that looks nicer and has a sort of cartoon effect to remind the user of the original Super Nintendo game."
        }
    },
